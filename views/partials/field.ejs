<p>
  <% foundFields.forEach(field=> { %>
    <div class="btn-group custom-field-btn">
      <a class="btn my-2 fff" href="/<%= _.lowerCase(field.fieldTitle) %>"
        style="background-color:<%= field.fieldColor %>; border-color:<%= field.fieldColor %>;">
        <%= field.fieldTitle %>
      </a>
      <button type="button" class="btn fff my-2 dropdown-toggle dropdown-toggle-split"
        style="background-color:<%= field.fieldColor %>; border-color:<%= field.fieldColor %>;"
        data-bs-toggle="dropdown" aria-expanded="false">
        <span class="visually-hidden">Toggle Dropdown</span>
      </button>
      <ul class="dropdown-menu">
        <li><a class="dropdown-item" href="#">Delete</a></li>
      </ul>
    </div>
    <% }) %>
</p>